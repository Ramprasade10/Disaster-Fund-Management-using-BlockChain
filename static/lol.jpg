  };

  initWall();

  window.addEventListener('resize', function(event) {
    if (window.isMobile) {
      return true;
    }
    return initWall();
  });

  window.addEventListener("DOMContentLoaded", function(event) {
    return window.initWall();
  });

}).call(this);
